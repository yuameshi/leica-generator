{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "isolatedModules": true,
        "types": [
            "vite/client"
        ]
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}
